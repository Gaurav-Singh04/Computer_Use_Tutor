generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  title       String
  description String   @default("")
  start       DateTime
  end         DateTime
  color       String   @default("#1976d2")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
